func do_this() -> int {
  x := 5;
  y := 3

  println("This is x when in the function: " + x);
  println("This is y when in the function: " + y);

  if x < y {
    println("This is true when in the function");
    return x + y;
  } else if x == y {
    return x + 2;
  } else {
    return y - x;
  }
}

println("The function do_this() returns: " + do_this());
