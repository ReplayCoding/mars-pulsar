func do_thing(bool unused) -> int {
     x := 1;
     y := 3;
     println("This is x when in the function: " + x);
     println("This is y when in the function: " + y);
     println("This is unused when in the function: " + unused);
     if x == y {
          println("x is equal to y");
          return x + y;
     }
}

println(do_thing(true));
println(do_thing(false));
